{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm gen:api && concurrently \"pnpm dev:next\" \"pnpm dev:watch\"",
    "dev:next": "next dev",
    "dev:watch": "nodemon --watch openapi/openapi.yml --exec pnpm gen:api",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check": "pnpm type-check && pnpm lint && pnpm format:check",
    "fix": "pnpm type-check && pnpm lint:fix && pnpm format",
    "gen:api": "pnpm exec orval --config orval.config.ts",
    "storybook": "storybook dev -p 6006",
    "storybook:docker": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "vrt:build": "docker compose -f compose.vrt.yml up -d --build",
    "vrt:up": "docker compose -f compose.vrt.yml up -d",
    "vrt:run": "docker exec vrt-storybook pnpm test:vrt",
    "vrt:update": "docker exec vrt-storybook pnpm test:vrt:update",
    "vrt:down": "docker compose -f compose.vrt.yml down",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:vrt": "concurrently \"pnpm test:vrt:pc\" \"pnpm test:vrt:sp\"",
    "test:vrt:update": "concurrently \"pnpm test:vrt:pc:update\" \"pnpm test:vrt:sp:update\"",
    "test:vrt:pc": "VRT_DEVICE=pc test-storybook --url http://localhost:6006",
    "test:vrt:sp": "VRT_DEVICE=sp test-storybook --url http://localhost:6006",
    "test:vrt:pc:update": "VRT_DEVICE=pc test-storybook --url http://localhost:6006 -u",
    "test:vrt:sp:update": "VRT_DEVICE=sp test-storybook --url http://localhost:6006 -u"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.0",
    "next": "16.1.5",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/nextjs": "^10.2.8",
    "@storybook/nextjs-vite": "^10.2.8",
    "@storybook/react": "^10.2.8",
    "@storybook/test-runner": "^0.24.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-image-snapshot": "^6.4.1",
    "@types/node": "^20.19.33",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^10.2.8",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^16.5.0",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^27.4.0",
    "nodemon": "^3.1.11",
    "orval": "^8.3.0",
    "prettier": "^3.8.1",
    "storybook": "^10.2.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.4"
  }
}
