# タイムゾーン設定
TZ=Asia/Tokyo

# バックエンドAPIの公開ポート
BACKEND_PORT=8080

# データベース接続情報
POSTGRES_USER=go_shisha
POSTGRES_PASSWORD=password
POSTGRES_DB=go_shisha_dev
POSTGRES_PORT=5432

# ログ設定（オプション、未設定時はdevelopment/DEBUGが使用されます）
APP_ENV=development
LOG_LEVEL=DEBUG

# フロントエンド URL（CORS設定用）
FRONTEND_URL=http://localhost:3000

# JWT認証設定（本番環境では必ず64文字以上の強力なランダム文字列に変更してください）
# 例: openssl rand -base64 64 | tr -d '\n'
JWT_SECRET=CHANGE_ME_WITH_STRONG_RANDOM_STRING_MUST_BE_AT_LEAST_64_CHARACTERS_LONG_PLACEHOLDER